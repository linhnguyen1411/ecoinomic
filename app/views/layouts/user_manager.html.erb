<!DOCTYPE html>
<html>
  <head>
    <title>Ecoinomic</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'user_manager', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'user_manager', 'data-turbolinks-track': 'reload' %>
  </head>
  <body class="dashboard">
    <div class="wrapper">
      <div class="sidebar">
        <%= render "layouts/users/menu" %>
      </div>
      <div class="main-panel">
        <%= render "layouts/users/header" %>
        <div class="content pt15">
          <div class="container-fluid">
            <div id="app">
              <%= yield %>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
